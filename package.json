{
  "name": "mongo2elastic",
  "version": "0.1.0",
  "description": "Sync MongoDB collections to Elasticsearch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": "git://github.com/smartprocure/mongo2elastic.git",
  "scripts": {
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc --declaration",
    "build:develop": "tsc --watch",
    "lint": "eslint src/**",
    "fmt": "prettier --ignore-path .gitignore --write './'"
  },
  "keywords": [
    "mongodb",
    "mongo",
    "collection",
    "elastic",
    "elasticsearch",
    "index",
    "sync",
    "change",
    "stream"
  ],
  "author": "GovSpend",
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "ioredis": "^5.2.2",
    "@elastic/elasticsearch": "^8.2.1"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "mongochangestream": "^0.12.0",
    "p-retry": "^5.1.1",
    "print-stats": "^0.2.0",
    "prom-utils": "^0.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
